
# Set these in Vercel Project > Settings > Environment Variables (do NOT commit secrets)
TURSO_URL=libsql://your-db.turso.io
TURSO_TOKEN=your_turso_token_here
# Optional: override channels JSON location
CHANNELS_JSON_URL=https://drive.google.com/uc?export=download&id=1y_baDMf3VVYrksEhbE2YQZ5td0REd89p
# Optional: cron-job.org API key and job id for reset (if you want auto-reset on failures)
CRON_API_KEY=
CRON_JOB_ID=
# Control concurrency (defaults to 1 to minimize memory/CPU on Vercel Hobby)
CONCURRENCY=1
